{
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true

  },
  "plugins": [
    "react",
    "jsx-a11y",
    "react-hooks",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "react/jsx-props-no-spreading": "warn",
    "indent": [
      "warn",
      2
    ],
    "semi": "error"
  },
  "extends": [
   "airbnb",
   "next",
   "prettier"
  ]
}
